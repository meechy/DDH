<div style="margin-bottom:30px;"></div>

<form name="y" style="margin-top:20px;border:2px solid #D1D1D1;border-radius:6px;padding:10px;" method="post" action="https://flash.duckduckgo.com/e.js" onsubmit="return validate_form ();">

<: l('Email') :> <small>(<: l('optional') :>)</small>:
<br>
<br><input type="text" name="from" size="20">

<div style="padding-top:20px;"></div>
<: l('Site name, e.g. %s or %s','DuckDuckGo','The New York Times') :>:
<br>
<br><input type="text" name="bang_site" size="20">

<div style="padding-top:20px;"></div>
<: l('Bang command, e.g. %s (usually is domain name, e.g. %s)','duckduckgo','nytimes') :>:
<br>
<br>!&nbsp;<input type="text" name="bang_command" size="10">

<div style="padding-top:20px;"></div>
<: l('Bang url, e.g. %s','http://duckduckgo.com/?q={{{s}}}') :>:
<br><small>&nbsp; <: l('Mark where the search goes with {{{s}}}') :></small>
<br><input type="text" name="bang_url" size="40">

<div style="padding-top:20px;"></div>
<: l('Bang category') :>:
<br>
<br>
<select name="bang_cat" onChange="fill_subcat(this.selectedIndex);">
  <option value=""><: l('Choose category') :></option>
  <option value="Tech"><: lp('bangcat','Tech') :></option>
  <option value="Shopping"><: lp('bangcat','Shopping') :></option>
  <option value="Research"><: lp('bangcat','Research') :></option>
  <option value="Online Services"><: lp('bangcat','Online Services') :></option>
  <option value="News"><: lp('bangcat','News') :></option>
  <option value="Multimedia"><: lp('bangcat','Multimedia') :></option>
  <option value="Entertainment"><: lp('bangcat','Entertainment') :></option>
</select>
<select name="bang_subcat" class="invisible"></select>

<div style="padding-top:20px;"></div>
<: l('Comments') :> <small>(<: l('optional') :>)</small>:
<br>
<br><textarea name="bang_comments" cols="40" rows="2"></textarea>

<input type="hidden" value="On" name="back">

<div style="padding-top:20px;"></div>
<input type="submit" value="<: l('Submit') :>">
<input type="hidden" name="go">

<div style="padding-top:40px;"></div>

</form>
