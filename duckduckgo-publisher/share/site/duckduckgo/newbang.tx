window.onload=function(){fill_subcat(d.y.bang_subcat.selectedIndex);}

function validate_form (){ 
    valid = true;
    missing = "";
    if (d.y.bang_site.value == ""){
        missing += "\t<: js(lp('newbang','Site name')) :>\n";
        valid = false;
    }
    if (d.y.bang_command.value == ""){
        missing += "\t<: js(lp('newbang','Bang command')) :>\n";
        valid = false;
    }
    if (d.y.bang_url.value == ""){
        missing += "\t<: js(lp('newbang','Bang url')) :>\n";
        valid = false;
    }
    if (d.y.bang_cat.value == ""){
        missing += "\t<: js(l('Category')) :>\n";
        valid = false;
    } 
    if (d.y.bang_subcat.value == ""){
        missing += "\t<: js(l('Subcategory')) :>\n";
        valid = false;
    }
    if (valid){
        return valid;
    }
    else {
        alert ("<: js(l('The following required fields were not filled:')) :>\n" + missing);
        return valid;
    }
}
